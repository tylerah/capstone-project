CREATE TABLE demographics (
	RaceEth DECIMAL, 
	Record DECIMAL, 
	Age DECIMAL, 
	Sex DECIMAL,
	Grade DECIMAL, 
	Hispanic DECIMAL, 
	white INT, 
	AIN INT,
    asian INT, 
	black INT, 
	PI INT, 
	Height DECIMAL, 
	Weight2 DECIMAL, 
	BMIPCT DECIMAL, 
	Weight DECIMAL,
    Stratum DECIMAL, 
	PSU DECIMAL);

SELECT * FROM demographics;

